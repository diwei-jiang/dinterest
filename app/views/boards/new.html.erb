<% provide(:title, 'New board') %>
<h1>New board</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@board) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :name %>
      <%= f.text_field :name %>

      <button class="btn btn-large btn-primary" name="commit" type="submit">
        Finish
      </button>
    <% end %>
  </div>
</div>
