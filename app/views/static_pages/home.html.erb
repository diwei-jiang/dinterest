<% if signed_in? %>
  <h3>New Pins Feed</h3>
  <%= render 'shared/feed' %>
<% else %>

<div class="center jumbotron">
  <h1>Welcome to Dinterest</h1>
  <h2>
    Dinterest is a Pinterest-like picture sharing hub.
  </h2> 
   <%= link_to "Sign up now!", signup_path, class: "btn btn-large btn-primary" %>
</div>
<% end %>